<div class="row" ng-controller="myCtrl">
    <p>{{firstName}}</p>
    <table class="table table-bordered">
        <thead>
        <tr>
            <td>Student Id</td>
            <td>FirstName</td>
            <td>LastName</td>
            <td>Age</td>
            <td colspan="2">Action</td>
        </tr>
        </thead>
        <tbody>
        <tr ng-repeat="x in students">
            <td>{{x.StudentId}}</td>
            <td>{{x.FirstName}}</td>
            <td>{{x.LastName}}</td>
            <td>{{x.Age}}</td>
            <td>@Html.ActionLink("Edit","Edit")</td>
            <td>
                <a href="#" ng-click="Delete(x)" style="color: red">x</a>
            </td>
        </tr>
        </tbody>
    </table>
</div>

<script src="~/Scripts/defaultjs.js"></script>
<div id="root">

</div>
